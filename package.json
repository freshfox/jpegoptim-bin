{
  "name": "jpegoptim-bin",
  "version": "8.0.1",
  "description": "jpegoptim wrapper that makes it seamlessly available as a local dependency",
  "license": "MIT",
  "repository": "imagemin/jpegoptim-bin",
  "type": "module",
  "exports": "./index.js",
  "author": {
    "name": "Shogo Sensui",
    "email": "shogosensui@gmail.com",
    "url": "github.com/1000ch"
  },
  "bin": {
    "jpegoptim": "cli.js"
  },
  "engines": {
    "node": "^14.13.1 || >=16.0.0"
  },
  "scripts": {
    "postinstall": "node lib/install.js",
    "test": "xo && ava --timeout=120s"
  },
  "files": [
    "cli.js",
    "index.js",
    "lib"
  ],
  "keywords": [
    "compress",
    "imagemin",
    "image",
    "img",
    "jpeg",
    "jpg",
    "minify",
    "optimize",
    "jpegoptim"
  ],
  "dependencies": {
    "bin-build": "^3.0.0",
    "bin-wrapper": "^4.1.0"
  },
  "devDependencies": {
    "ava": "^5.3.0",
    "bin-check": "^4.1.0",
    "compare-size": "^3.0.0",
    "execa": "^7.1.1",
    "tempy": "^3.0.0",
    "xo": "^0.54.2"
  },
  "xo": {
    "rules": {
      "unicorn/prefer-top-level-await": "off"
    }
  }
}
